{
  "name": "vue3-cli-template",
  "version": "1.0.1",
  "private": false,
  "author": "ErrorJE",
  "main": "index.js",
  "scripts": {
    "init": "npx husky install",
    "clear": "rimraf node_modules&&npm install --registry=https://registry.npm.taobao.org&&cnpm i",
    "mock": "json-server --watch __json_server_mock__/db.json --port 6666",
    "serve": "vue-cli-service serve --mode mock",
    "watch": "vue-cli-service build --watch --no-clean --mode development",
    "build:develop": "vue-cli-service build --mode development",
    "build:master": "vue-cli-service build",
    "dust-build:develop": "npm run build:develop",
    "dust-build:master": "npm run build:master",
    "release": "standard-version",
    "commit": "git add -A && git-cz",
    "update:auto": "ncu -u --reject compression-webpack-plugin,filemanager-webpack-plugin,sass,sass-loader,postcss,svgo --registry https://registry.npm.taobao.org&&npm install --registry=https://registry.npm.taobao.org&&cnpm i",
    "update:plus": "npm-check -u",
    "svgo": "svgo -f src/assets/icons/svg --config=src/assets/icons/svgo.yml",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o public/storybook-static"
  },
  "dependencies": {
    "autoprefixer": "^10.3.1",
    "axios": "^0.21.1",
    "core-js": "^3.16.0",
    "element-plus": "^1.0.2-beta.65",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "nprogress": "^0.2.0",
    "postcss": "^8.3.6",
    "postcss-preset-env": "^6.7.0",
    "qs": "^6.10.1",
    "tailwindcss": "^2.2.7",
    "vue": "^3.1.5",
    "vue-i18n": "^9.1.7",
    "vue-router": "^4.0.10",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/vue3": "^6.3.6",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-router": "^4.5.13",
    "@vue/cli-plugin-typescript": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-plugin-vuex": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.1.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "commitizen": "^4.2.4",
    "compression-webpack-plugin": "^6.1.1",
    "css-loader": "^5.2.7",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.15.1",
    "filemanager-webpack-plugin": "^3.1.1",
    "husky": "^7.0.1",
    "json-server": "^0.16.3",
    "lint-staged": "^11.1.1",
    "postcss-loader": "^4.3.0",
    "prettier": "^2.3.2",
    "sass": "^1.37.2",
    "sass-loader": "^10.2.0",
    "standard-version": "^9.3.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.4.0",
    "stylelint-order": "^4.1.0",
    "svg-sprite-loader": "^6.0.9",
    "svgo": "^1.3.2",
    "typescript": "^4.3.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue-jest": "^5.0.0-0",
    "vue-loader": "^16.4.1",
    "vue-loader-v16": "^16.0.0-beta.5.4",
    "webpackbar": "^5.0.0-3"
  },
  "standard-version": {
    "infile": "docs/CHANGELOG.md",
    "scripts": {
      "posttag": "git push --follow-tags origin master"
    },
    "changelogHeader": "# VUE3_CLI_TEMPLATE \n",
    "commitAll": true
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "npx --no-install commitlint --edit $1"
    }
  },
  "lint-staged": {
    "src/**/*.{js, vue}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.{scss, less, css}": [
      "stylelint --fix",
      "prettier --write",
      "stylelint"
    ]
  },
  "keywords": [
    "vue3",
    "vue-cli4",
    "typescript",
    "storybook",
    "tailwindcss",
    "tsx"
  ],
  "license": "ISC"
}
